<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.test.clientmoudle">

    <application
        android:name="com.app.test.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/jr_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 网上药店icon -->
        <activity-alias

            android:name="cn.jianke.dynamicchangeicon.jkmall"
            android:enabled="false"
            android:icon="@drawable/jr_icon"
            android:label="今日头条(恢复版)"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity-alias>
        <!-- 健客医生icon -->
        <activity-alias
            android:name="cn.jianke.dynamicchangeicon.doctor"
            android:enabled="false"
            android:icon="@drawable/jrdp_icon"
            android:label="今日头条(护盾版)"
            android:targetActivity=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity-alias>
    </application>

</manifest>